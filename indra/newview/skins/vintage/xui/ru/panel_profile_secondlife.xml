		Сбросьте объект из инвентаря сюда.
	</text>
	<layout_stack name="buttonstack">
		<layout_panel name="left_buttonstack">
			<button name="show_on_map_btn" label="На Карте" label_selected="На Карте" tool_tip="Найдите Жителя на карте"/>
			<button name="pay" label="Платить" label_selected="Платить" tool_tip="Выплатить деньги Жителю"/>
		</layout_panel>
		<layout_panel name="middle_buttonstack">
			<button name="teleport" label="Предложить Телепорт" label_selected="Предложить Телепорт" tool_tip="Предложить Телепорт Жителю"/>
			<button name="im" label="Быстрое Сообщение" label_selected="Быстрое Сообщение" tool_tip="Открыть сеанс мгновенных сообщений"/>
		</layout_panel>
		<layout_panel name="right_buttonstack">
			<button name="add_friend" label="Добавить Друга" label_selected="Добавить Друга" tool_tip="Предложить дружбу Жителю"/>
			<button name="remove_friend" label="Удалить друга" label_selected="Удалить друга" tool_tip="Прекратить дружбу с жителем"/>
			<button name="block" label="Блокировать" tool_tip="Заблокировать этого Жителя"/>
			<button name="unblock" label="Разблокировать" tool_tip="Разблокировать этого Жителя"/>
		</layout_panel>
	</layout_stack>
	<check_box name="show_in_search" label="Показать в поиске" tool_tip="Пусть люди видят вас в результатах поиска"/>
	<button name="save_description_changes" label="Сохранить"/>
	<button name="discard_description_changes" label="Отклонить"/>
