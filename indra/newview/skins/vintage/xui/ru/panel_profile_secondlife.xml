		</layout_panel>
		<layout_panel name="label_stack">
			<icon name="can_see_online" tool_tip="Друг может видеть мой онлайн-статус"/>
			<icon name="cant_see_online" tool_tip="Друг не может видеть мой онлайн-статус"/>
