  <tab_container name="tabs">
    <panel label="Nearby" name="nearby_panel">
      <tab_container name="tabcont_radarpeople_main">
        <panel name="panel_radar" label="List" />
        <panel name="radarmap_options_panel" label="Options">
          <scroll_container label="Options" name="radarmap_options_scroll_container">
            <container_view name="radarmap_options_container_view" label="Options">
              <panel name="radarmap_options_scrolled_panel">
                <check_box label="Show Minimap above Radar" name="ShowRadarMinimap" />
                <check_box label="Show muted and blacklisted Avatars" name="FSRadarShowMutedAndDerendered" />
                <check_box label="Report Entering Draw Range" name="RadarReportDrawRangeEnter" />
                <check_box label="Report Leaving Draw Range" name="RadarReportDrawRangeLeaving" />
                <check_box label="Report Entering Chat Range" name="RadarReportChatRangeEnter" />
                <check_box label="Report Leaving Chat Range" name="RadarReportChatRangeLeave" />
                <check_box label="Report Entering your Region" name="RadarReportSimRangeEnter" />
                <check_box label="Report Leaving your Region" name="RadarReportSimRangeLeave" />
                <check_box label="Report Enter Alerts to Scripts" name="RadarEnterChannelAlert" />
                <check_box label="Report Exit Alerts to Scripts" name="RadarLeaveChannelAlert" />
                <check_box label="Enhance radar with LSL-Client Bridge" tool_tip="If this is enabled, radar functions responsible for determining avatar locations at high altitudes are assisted by the LSL-Client Bridge, particularly when they're beyond draw distance." name="FSRadarEnhanceByBridge" />
                <check_box label="Color names based on distance" name="FSRadarColorNamesByDistance" />

                <check_box label="Limit range to:" tool_tip="Limit Results to specific Radius" name="LimitRadarByRange" />
