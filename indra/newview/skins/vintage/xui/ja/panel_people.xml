                  <combo_box.item name="RadarNameFormat_user" value="1" label="Username only" />
                  <combo_box.item name="RadarNameFormat_displayuser" value="2" label="Display Name (Username)" />
                  <combo_box.item name="RadarNameFormat_userdisplay" value="3" label="Username (Display Name)" />
                </combo_box>

                <text name="columns_label" value="Displayed Columns:" />
                <check_box label="Voice" name="column_voice" />
                <check_box label="Time" name="column_seen" />

                <check_box label="Age" name="column_age" />
                <check_box label="Same Region" name="column_region" />
                <check_box label="Typing Status" name="column_typing_status" />
                <check_box label="Sitting Status" name="column_sitting_status" />
                <check_box label="Flags" name="column_flags" />
                <check_box label="Notes" name="notes_flags" />
              </panel>
            </container_view>
          </scroll_container>
        </panel>
