<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel name="panel_profile_classifieds" label="Classified">
  <string name="no_classifieds" value="No Classifieds" />
  <layout_stack name="main_stack">
    <layout_panel name="buttons_header">
      <button name="new_btn" label="New..." tool_tip="Create a new classified at the current location" />
      <button name="delete_btn" label="Delete..." tool_tip="Delete currently selected classified" />
    </layout_panel>
    <layout_panel name="main_body">
      <tab_container name="tab_classifieds" />
      <layout_stack name="indicator_stack">
        <layout_panel name="indicator_spacer_left">
        </layout_panel>
        <layout_panel name="buttons_header">
          <loading_indicator name="progress_indicator" />
        </layout_panel>
        <layout_panel name="indicator_spacer_right">
        </layout_panel>
      </layout_stack>
      <text name="classifieds_panel_text">Loading...</text>
    </layout_panel>
  </layout_stack>
</panel>
