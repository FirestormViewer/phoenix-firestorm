<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<toolbar_view name="toolbar view">
  <layout_stack name="bottom_toolbar_stack">
  <layout_panel name="bottom_chat_bar_panel">
  <layout_stack name="chat_bar_utility_bar_stack">
  <layout_panel name="chat_panel">
  <layout_stack name="bottom_chat_stack">
  <layout_panel name="bottom_chat_bar_panel">
  <button name="local_chat_button" tool_tip="Pokaż/ukryj okno czatu lokalnego (CTRL+H)" label="Czat lokalny" />
  <fs_nearby_chat_control name="default_chat_bar" label="Na czat lokalny" />
  </layout_panel>
  </layout_stack>
  </layout_panel>
  <layout_panel name="utility_bar_layout_panel">
  <panel name="utility_bar_panel">
  <button name="utility_parcel_audio_stream_button" tool_tip="Odtwórz/Wstrzymaj muzykę strumieniową na działce" />
  <button name="utility_parcel_media_button" tool_tip="Odtwórz/Wstrzymaj media na działce" />
  <slider name="System Volume" tool_tip="Dostrój głośność" />
  <button name="mute_audio" tool_tip="Wycisz dźwięki" />
  <button name="show_volume_controls_button" tool_tip="Pokaż kontrolę głośności" />
  </panel>
  </layout_panel>
  <layout_panel name="voice_chat_controls_layout_panel">
  <panel name="voice_chat_controls_panel">
  <button name="show_voice_controls_button" tool_tip="Pokaż rozmowy głosowe w pobliżu" />
  <button name="utility_talk_button" tool_tip="Naciśnij przycisk aby mówić. Kliknij na kłódkę aby ustawić naciśnij-by-mówić." label="Mów" />
  <button name="utility_push_to_talk_lock_button" tool_tip="Przełącza funkcję naciśnij-by-mówić" />
  <nearby_voice_monitor name="nearby_voice_output_monitor" tool_tip="Obecna aktywność kanału głosowego" />
  </panel>
  </layout_panel>
  <layout_panel name="quick_preferences_button_layout_panel">
  <panel name="quick_preferences_button_panel">
  <button name="enable_disable_ao_button" tool_tip="Włącz/wyłącz Animatora w kliencie" />
  <button name="show_ao_interface_button" tool_tip="Pokaż/ukryj interfejs Animatora w kliencie" />
  <button name="show_quick_prefs_button" tool_tip="Podręczne ustawienia dla często zmienianych opcji" />
  </panel>
  </layout_panel>
  </layout_stack>
  </layout_panel>
  <layout_panel name="bottom_toolbar_panel">
    <button name="open_chat_bar_button" tool_tip="Pokaż pasek czatu (Enter)" />
  </layout_panel>
  </layout_stack>
</toolbar_view>
