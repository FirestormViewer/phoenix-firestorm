<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 default_tab_group="1"
 follows="left|bottom|right"
 height="25"
 layout="topleft"
 left="0"
 name="chat_bar"
 top="21"
 width="308">
    <line_editor
     border_style="line"
     border_thickness="1"
     follows="left|right"
     height="23"
     label="Click here to chat."
     layout="topleft"
     left_delta="3"
     left="0"
     max_length_bytes="1023"
     name="chat_box"
     spellcheck="true"
     text_pad_left="5"
     text_pad_right="25"
     tool_tip="Press Enter to say, Ctrl+Enter to shout"
     top="2"
     width="303" />
    <output_monitor
     auto_update="true"
     follows="right"
     draw_border="false"
     height="16"
     layout="topleft"
     left_pad="-30"
     mouse_opaque="true"
     name="chat_zone_indicator"
     top="6"
     visible="true"
     width="20" />
</panel>
