<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
    legacy_header_height="18"
    can_resize="false"
    can_dock="true"
    can_close="false"
    height="220"
    width="260"
    layout="topleft"
    name="quickprefs"
    help_topic="fs_quick_preferences"
    save_rect="true"
    save_visibility="true"
    single_instance="true"
    save_dock_state="true"
    title="Quick Preferences">
<panel
    name="quickprefs"
    left="0"
    top="16"
    width="260"
    height="200"
    follows="all"
    layout="topleft">
        <text
            type="string"
            length="1"
            follows="left|top"
            height="10"
            layout="topleft"
            left="5"
            name="Draw Distance"
            tool_tip=""
            mouse_opaque="false"
            top="10"
            width="90">
            Draw Distance:
        </text>
        <slider
            can_edit_text="true"
            control_name="RenderFarClip"
            decimal_digits="0"
            follows="left|top"
            height="15"
            increment="8"
            initial_value="160"
            layout="topleft"
            left_delta="93"
            max_val="1024"
            min_val="32"
            name="DrawDistance"
            top_delta="-2"
            width="150"
            label_width="3"/>
        <text
            type="string"
            length="1"
            follows="left|top"
            height="10"
            layout="topleft"
            left="5"
            name="Particles"
            tool_tip=""
            mouse_opaque="false"
            top_delta="20"
            width="90">
            Max Particles:
        </text>
        <slider
            can_edit_text="true"
            control_name="RenderMaxPartCount"
            decimal_digits="0"
            follows="left|top"
            height="15"
            increment="256"
            initial_value="4096"
            layout="topleft"
            left_delta="93"
            max_val="8192"
            min_val="0"
            name="RenderMaxPartCount"
            top_delta="-2"
            width="150"
            label_width="3"/>
        <text
            type="string"
            length="1"
            follows="left|top"
            height="10"
            layout="topleft"
            left="5"
            name="Avatar Physics LOD"
            tool_tip=""
            mouse_opaque="false"
            top_delta="20"
            width="90">
            Av Physics LOD:
        </text>
        <slider
            can_edit_text="true"
            control_name="RenderAvatarPhysicsLODFactor"
            decimal_digits="2"
            follows="left|top"
            height="15"
            increment="0.05"
            initial_value="1"
            layout="topleft"
            left_delta="93"
            max_val="1"
            min_val="0"
            name="RenderAvatarPhysicsLOD"
            top_delta="-2"
            width="146"
            label_width="3"/>
        <text
            type="string"
            length="1"
            follows="left|top"
            height="10"
            layout="topleft"
            left="5"
            name="LOD Factor"
            tool_tip=""
            mouse_opaque="false"
            top_delta="20"
            width="90">
            Objects LOD:
        </text>
        <slider
            can_edit_text="true"
            control_name="RenderVolumeLODFactor"
            decimal_digits="2"
            follows="left|top"
            height="15"
            increment="0.1"
            initial_value="2"
            layout="topleft"
            left_delta="82"
            max_val="4"
            min_val="0"
            name="RenderVolumeLODFactor"
            top_delta="-2"
            width="157"
            label_width="14"/>
        <text
            type="string"
            length="1"
            follows="left|top"
            height="10"
            layout="topleft"
            left="5"
            name="Max Avatars"
            tool_tip=""
            mouse_opaque="false"
            top_delta="20"
            width="90">
            Max Avatars:
        </text>
        <slider
            can_edit_text="true"
            control_name="RenderAvatarMaxVisible"
            decimal_digits="0"
            follows="left|top"
            height="15"
            increment="1"
            initial_value="12"
            layout="topleft"
            left_delta="79"
            max_val="50"
            min_val="1"
            name="RenderAvatarMaxVisible"
            top_delta="-2"
            width="150"
            label_width="17"/>
        <text
            type="string"
            length="1"
            follows="left|top"
            height="10"
            layout="topleft"
            left="5"
            name="Height Offset"
            tool_tip=""
            mouse_opaque="false"
            top_delta="20"
            width="90">
            Height Offset:
        </text>
        <slider
            can_edit_text="true"
            control_name="AvatarZOffset"
            decimal_digits="2"
            follows="left|top"
            height="15"
            increment="0.05"
            initial_value="0"
            layout="topleft"
            left_delta="79"
            max_val="2"
            min_val="-2"
            name="AvatarZOffset"
            top_delta="-2"
            width="164"
            label_width="17"/>
        <text
            type="string"
            length="1"
            follows="left|top"
            height="10"
            layout="topleft"
            left="5"
            name="WL Sky"
            tool_tip=""
            mouse_opaque="false"
            top_delta="27"
            width="90">
            WL Sky:
        </text>
        <combo_box
            height="20"
            layout="topleft"
            left_delta="58"
            name="WLPresetsCombo"
            top_delta="-4"
            width="150" />
        <button
            enabled="true"
            name="WLPrevPreset"
            follows="left|bottom"
            height="20"
            width="20"
            bottom_delta="-0"
            left_delta="152"
            label=""
            label_selected=""
            image_overlay="Arrow_Left_Off"
            scale_image="true" />
        <button
            enabled="true"
            name="WLNextPreset"
            follows="left|bottom"
            height="20"
            width="20"
            bottom_delta="-0"
            left_delta="21"
            label=""
            label_selected=""
            image_overlay="Arrow_Right_Off"
            scale_image="true" />
        <text
            type="string"
            length="1"
            follows="left|top"
            height="10"
            layout="topleft"
            left="5"
            name="WL Water"
            tool_tip=""
            mouse_opaque="false"
            top_delta="25"
            width="90">
            WL Water:
        </text>
        <combo_box
            height="20"
            layout="topleft"
            left_delta="58"
            name="WaterPresetsCombo"
            top_delta="-4"
            width="150" />
        <button
            enabled="true"
            name="WWPrevPreset"
            follows="left|bottom"
            height="20"
            width="20"
            bottom_delta="-0"
            left_delta="152"
            label=""
            label_selected=""
            image_overlay="Arrow_Left_Off"
            scale_image="true" />
        <button
            enabled="true"
            name="WWNextPreset"
            follows="left|bottom"
            height="20"
            width="20"
            bottom_delta="-0"
            left_delta="21"
            label=""
            label_selected=""
            image_overlay="Arrow_Right_Off"
            scale_image="true" />
        <icon
            height="15"
            image_name="icon_day_cycle_12am.tga"
            layout="topleft"
            left="10"
            top_pad="5"
            name="SkyDayCycle"
            use_draw_context_alpha="false"
            width="220"
            tool_tip="Change the time of the day"/>
        <multi_slider
            can_edit_text="true"
            control_name="WLSunPos"
            decimal_digits="0"
            follows="bottom"
            height="10"
            increment="0.0833333"
            initial_value="0"
            layout="topleft"
            left="10"
            max_sliders="1"
            max_val="24"
            name="WLSunPos"
            show_text="false"
            top_pad="-1"
            width="220" />
        <button
            enabled="true"
            name="UseRegionWL"
            follows="left|bottom"
            height="20"
            width="20"
            bottom_delta="-3"
            left_pad="5"
            label=""
            label_selected=""
            image_overlay="StopReload_Off"
            scale_image="true"
            tool_tip="Reset the WindLight options to region default" />
</panel>
</floater>
