<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
    positioning="centered"
    legacy_header_height="18"
    can_resize="false"
    can_dock="false"
    can_close="true"
    height="290"
    width="260"
    layout="topleft"
    name="quickprefs"
    help_topic="fs_quick_preferences"
    save_rect="true"
    save_visibility="true"
    single_instance="true"
    save_dock_state="false"
    title="Quick Preferences">
  <panel
      name="quickprefs"
      left="0"
      top="16"
      width="260"
      height="267"
      follows="all"
      layout="topleft">
    <text
        type="string"
        length="1"
        follows="left|top"
        height="10"
        layout="topleft"
        left="5"
        name="Draw Distance"
        tool_tip="Change your Draw Distance"
        mouse_opaque="false"
        top="10"
        width="90">
      Draw Distance:
    </text>
    <slider
        can_edit_text="true"
        control_name="RenderFarClip"
        decimal_digits="0"
        follows="left|top"
        height="15"
        increment="8"
        initial_value="160"
        layout="topleft"
        left_delta="93"
        max_val="1024"
        min_val="32"
        name="DrawDistance"
        top_delta="-2"
        width="150"
        label_width="3"/>
    <!--<FS:HG> FIRE-6340, FIRE-6567 - Setting Bandwidth issues -->
    <text
        type="string"
        length="1"
        follows="left|top"
        height="10"
        layout="topleft"
        left="5"
        name="Maximum bandwidth"
        tool_tip="Set your Network bandwidth in Kbps (kilobits per second)"
        mouse_opaque="false"
        top_delta="20"
        width="90">
      Max Bandwidth:
    </text>
    <slider
        can_edit_text="true"
        control_name="ThrottleBandwidthKBPS"
        decimal_digits="0"
        follows="left|top"
        height="15"
        increment="100"
        initial_value="500"
        layout="topleft"
        left_delta="99"
        max_val="3000"
        min_val="100"
        name="max_bandwidth"
        top_delta="-2"
        width="144" />
    <!--</FS:HG> FIRE-6340, FIRE-6567 - Setting Bandwidth issues -->
    <text
        type="string"
        length="1"
        follows="left|top"
        height="10"
        layout="topleft"
        left="5"
        name="Particles"
        tool_tip="Amount of particles to render"
        mouse_opaque="false"
        top_delta="20"
        width="90">
      Max Particles:
    </text>
    <slider
        can_edit_text="true"
        control_name="RenderMaxPartCount"
        decimal_digits="0"
        follows="left|top"
        height="15"
        increment="256"
        initial_value="4096"
        layout="topleft"
        left_delta="93"
        max_val="8192"
        min_val="0"
        name="RenderMaxPartCount"
        top_delta="-2"
        width="150"
        label_width="3"/>
    <text
          type="string"
          length="1"
          follows="left|top"
          height="10"
          layout="topleft"
          left="5"
          name="Avatar Physics LOD"
          tool_tip="Controls level of detail of avatar physics (such as breast jiggle)"
          mouse_opaque="false"
          top_delta="20"
          width="90">
      Av Physics LOD:
    </text>
    <slider
        can_edit_text="true"
        control_name="RenderAvatarPhysicsLODFactor"
        decimal_digits="2"
        follows="left|top"
        height="15"
        increment="0.05"
        initial_value="1"
        layout="topleft"
        left_delta="93"
        max_val="1"
        min_val="0"
        name="RenderAvatarPhysicsLOD"
        top_delta="-2"
        width="147"
        label_width="3"/>
    <text
        type="string"
        length="1"
        follows="left|top"
        height="10"
        layout="topleft"
        left="5"
        name="LOD Factor"
        tool_tip="Controls level of detail of prims, sculpts, meshes at far distances. Impacts render speed and memory use."
        mouse_opaque="false"
        top_delta="20"
        width="90">
      Object LOD:
    </text>
    <slider
        can_edit_text="true"
        control_name="RenderVolumeLODFactor"
        decimal_digits="2"
        follows="left|top"
        height="15"
        increment="0.1"
        initial_value="2"
        layout="topleft"
        left_delta="82"
        max_val="4"
        min_val="0"
        name="RenderVolumeLODFactor"
        top_delta="-2"
        width="158"
        label_width="14"/>
    <text
        type="string"
        length="1"
        follows="left|top"
        height="10"
        layout="topleft"
        left="5"
        name="Max Avatars"
        tool_tip="How many avatars to fully render on screen. Lowering this greatly improves FPS in crowded situations. Requires Avatar Imposters to be on. [Default 12]"
        mouse_opaque="false"
        top_delta="20"
        width="90">
      Max Avatars:
    </text>
    <slider
        can_edit_text="true"
        control_name="RenderAvatarMaxVisible"
        decimal_digits="0"
        follows="left|top"
        height="15"
        increment="1"
        initial_value="12"
        layout="topleft"
        left_delta="79"
        max_val="65"
        min_val="1"
        name="RenderAvatarMaxVisible"
        top_delta="-2"
        width="152"
        label_width="17"/>
    <text
        type="string"
        length="1"
        follows="left|top"
        height="10"
        layout="topleft"
        left="5"
        name="Height Offset"
        tool_tip="Use this to manipulate, to a degree, your avatar bounding box. This can be used to raise you up or down without altering your shape, typically for adjusting your position when seated to more naturally line up with the sitting surface."
        mouse_opaque="false"
        top_delta="20"
        width="90">
      Av Height Offset:
    </text>
    <slider
        can_edit_text="true"
        control_name="AvatarZOffset"
        decimal_digits="2"
        follows="left|top"
        height="15"
        increment="0.05"
        initial_value="0"
        layout="topleft"
        left_delta="79"
        max_val="2"
        min_val="-2"
        name="AvatarZOffset"
        top_delta="-2"
        width="165"
        label_width="17"/>
    <text
            type="string"
            length="1"
            follows="left|top"
            height="0"
            layout="topleft"
            left="5"
            name="name_tags_textbox"
			top_pad="5"
            width="400">
      Show Av Nametags:
    </text>
    <radio_group
         control_name="AvatarNameTagMode"
         height="20"
         layout="topleft"
         left="55"
         top_pad="0"
         name="Name_Tag_Preference">
      <radio_item
         label="On"
         name="radio"
         top_delta="20"
         layout="topleft"
         height="16"
         left="72"
         value="1"
         width="75" />
      <radio_item
         label="Off"
         left_pad="-10"
         layout="topleft"
         top_delta="0"
         height="16"
         name="radio2"
         value="0"
         width="75" />
    </radio_group>
    
    <text
		type="string"
		length="1"
		follows="left|top"
		height="0"
		layout="topleft"
		left="5"
		name="lookat_textbox"
		top_pad="0"
		width="400">
        Show Lookat Marks:
    </text>
    <radio_group
		 control_name="DebugLookAt"
		 height="20"
		 layout="topleft"
		 left="55"
		 top_pad="0"
		 name="showlookat">
      <radio_item
         label="On"
         name="radio"
         top_delta="20"
         layout="topleft"
         height="16"
         left="72"
         value="1"
         width="75" />
      <radio_item
         label="Off"
         left_pad="-10"
         layout="topleft"
         top_delta="0"
         height="16"
         name="radio2"
         value="0"
         width="75" />
	</radio_group>

    <text
		type="string"
		length="1"
		follows="left|top"
		height="0"
		layout="topleft"
		left="5"
		name="ShowColor_QP_Label"
		top_pad="0"
		width="400">
        Show Color @Cursor:
    </text>	
    <radio_group
		 control_name="DebugShowColor"
		 height="20"
		 layout="topleft"
		 left="55"
		 top_pad="0"
		 name="Show_Color_Under_Cursor">
      <radio_item
         label="On"
         name="radio5"
         top_delta="20"
         layout="topleft"
         height="16"
         left="72"
         value="1"
         width="75" />
      <radio_item
         label="Off"
         left_pad="-10"
         layout="topleft"
         top_delta="0"
         height="16"
         name="radio6"
         value="0"
         width="75" />
	</radio_group>


         
    <text
        type="string"
        length="1"
        follows="left|top"
        height="10"
        layout="topleft"
        left="5"
        name="WL Sky"
        tool_tip="Windlight presets for your sky"
        mouse_opaque="false"
        top_delta="22"
        width="90">
      Sky:
    </text>
    <combo_box
        height="20"
        layout="topleft"
        left_delta="58"
        name="WLPresetsCombo"
        top_delta="-4"
        width="150" />
    <button
        enabled="true"
        name="WLPrevPreset"
        follows="left|bottom"
        height="20"
        width="20"
        bottom_delta="-0"
        left_delta="152"
        label=""
        label_selected=""
        image_overlay="Arrow_Left_Off"
        scale_image="true" />
    <button
        enabled="true"
        name="WLNextPreset"
        follows="left|bottom"
        height="20"
        width="20"
        bottom_delta="-0"
        left_delta="21"
        label=""
        label_selected=""
        image_overlay="Arrow_Right_Off"
        scale_image="true" />
    <text
        type="string"
        length="1"
        follows="left|top"
        height="10"
        layout="topleft"
        left="5"
        name="WL Water"
  tool_tip="Windlight presets for your water"
        mouse_opaque="false"
        top_delta="25"
        width="90">
      Water:
    </text>
    <combo_box
        height="20"
        layout="topleft"
        left_delta="58"
        name="WaterPresetsCombo"
        top_delta="-4"
        width="150" />
    <button
        enabled="true"
        name="WWPrevPreset"
        follows="left|bottom"
        height="20"
        width="20"
        bottom_delta="-0"
        left_delta="152"
        label=""
        label_selected=""
        image_overlay="Arrow_Left_Off"
        scale_image="true" />
    <button
        enabled="true"
        name="WWNextPreset"
        follows="left|bottom"
        height="20"
        width="20"
        bottom_delta="-0"
        left_delta="21"
        label=""
        label_selected=""
        image_overlay="Arrow_Right_Off"
        scale_image="true" />
    <icon
        height="15"
        image_name="icon_day_cycle_12am.tga"
        layout="topleft"
        left="10"
        top_pad="5"
        name="SkyDayCycle"
        use_draw_context_alpha="false"
        width="220"
        tool_tip="Change the time of the day"/>
    <multi_slider
        can_edit_text="true"
        control_name="WLSunPos"
        decimal_digits="0"
        follows="bottom"
        height="10"
        increment="0.0833333"
        initial_value="0"
        layout="topleft"
        left="10"
        max_sliders="1"
        max_val="24"
        name="WLSunPos"
        show_text="false"
        top_pad="-1"
        width="220" />
    <button
        enabled="true"
        name="UseRegionWL"
        follows="left|bottom"
        height="20"
        width="20"
        bottom_delta="-3"
        left_pad="5"
        label=""
        label_selected=""
        image_overlay="StopReload_Off"
        scale_image="true"
        tool_tip="Reset the WindLight options to region default" />
  </panel>
</floater>
