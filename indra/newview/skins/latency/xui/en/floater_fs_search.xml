 height="590"
 width="780"
 layout="topleft"
 can_resize="true"
 default_tab_group="1"
 help_topic="fs_search"
 legacy_header_height="0"
 min_height="590"
 min_width="660"
 positioning="centered"
 save_rect="true"
 single_instance="true"
 title="Firestorm Search"
>
    <!-- Strings -->
    <floater.string
     name="string.location"
    >
        Location: [LOCATION]
    </floater.string>
    <floater.string
     name="string.traffic"
    >
        Traffic: [DWELL]
    </floater.string>
    <floater.string
     name="string.area"
    >
        Area: [AREA]
    </floater.string>
    <floater.string
     name="string.members"
    >
        Members: [MEMBER_COUNT]
    </floater.string>
    <floater.string
     name="string.founder"
    >
        Founder: [FOUNDER]
    </floater.string>
    <floater.string
     name="string.age"
    >
        Age: [AGE]
    </floater.string>
    <floater.string
     name="string.partner"
    >
        Partner: [PARTNER]
    </floater.string>
    <floater.string
     name="string.listing_price"
    >
        Listing Price: [LISTING_PRICE]
    </floater.string>
    <floater.string
     name="string.slurl"
    >
        [SLURL]
    </floater.string>
    <floater.string
     name="string.duration"
    >
        Duration: [DURATION]
    </floater.string>
    <floater.string
     name="string.covercharge"
    >
        Cover: [COVERCHARGE]
    </floater.string>
    <!-- Tab time -->
    <tab_container
     name="ls_tabs"
     top="1"
     left="0"
     height="585"
     width="780"
     follows="all"
     layout="topleft"
     tab_min_width="90"
     tab_position="top"
    >
        <panel
         name="panel_ls_web"
         label="Websearch"
         layout="topleft"
         class="panel_ls_web"
         filename="panel_fs_search_legacy_web.xml"
        />
        <panel
         name="panel_ls_classifieds"
         label="Classifieds"
         layout="topleft"
         class="panel_ls_classifieds"
         filename="panel_fs_search_legacy_classifieds.xml"
        />
        <panel
         name="panel_ls_events"
         label="Events"
         layout="topleft"
         class="panel_ls_events"
         filename="panel_fs_search_legacy_events.xml"
        />
        <panel
         name="panel_ls_land"
         label="Land Sales"
         layout="topleft"
         class="panel_ls_land"
         filename="panel_fs_search_legacy_land.xml"
