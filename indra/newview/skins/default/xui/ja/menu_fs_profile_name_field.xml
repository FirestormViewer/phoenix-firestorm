<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<toggleable_menu
 name="CopyMenu">
    <menu_item_call
     label="Copy Display Name"
     layout="topleft"
     name="copy_display">
      <on_click function="Profile.Commit" parameter="copy_display_name"/>
    </menu_item_call>
    <menu_item_call
     label="Copy Agent Name"
     layout="topleft"
     name="copy_name">
        <on_click function="Profile.Commit" parameter="copy_username"/>
    </menu_item_call>
    <menu_item_call
     label="Copy UUID"
     layout="topleft"
     name="copy_id">
      <on_click function="Profile.Commit" parameter="copy_user_id"/>
    </menu_item_call>  
    <menu_item_call
     label="Copy URI"
     layout="topleft"
     name="copy_uri">
      <on_click function="Profile.Commit" parameter="copy_uri"/>
    </menu_item_call>  
</toggleable_menu>
