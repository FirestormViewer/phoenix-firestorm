<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater name="animation_explorer" title="アニメーションエクスプローラー">

  <scroll_list name="animation_list">
    <scroll_list.column label="再生者" name="played_by" />
    <scroll_list.column label="再生時間" name="played" />
    <scroll_list.column label="優先度" name="priority" width="60" />
    <scroll_list.column label="タイムスタンプ" name="timestamp" width="-1" />
    <scroll_list.column label="アニメーションＩＤ" name="animation_id" width="-1" />
    <scroll_list.column label="オブジェクトＩＤ" name="object_id" width="-1" />
  </scroll_list>

  <view_border name="animation_preview" />

  <panel name="buttons_panel">
    <button label="停止" name="stop_btn" />
    <button label="停止して取り消す" name="stop_and_revoke_btn" />
    <button label="ブラックリスト" name="blacklist_btn" />
    <check_box label="所有していないアニメーション" name="no_owned_animations_check" tool_tip="ここをチェックすると、ＡＯアニメーションやシステムデフォルトのアニメーションなど、あなたのアバターが所有しているすべてのアニメーションが表示されなくなります。" />
  </panel>
</floater>