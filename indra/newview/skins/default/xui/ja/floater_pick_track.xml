<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater name="track picker" title="選択：トラック">
	<layout_stack name="adjuster">
		<layout_panel name="pnl_desc">
			<text name="select_description">
				スカイのソースを選択：
			</text>
		</layout_panel>
		<layout_panel name="pnl_traks">
			<radio_group name="track_selection">
				<radio_item label="スカイ４[ALT]" name="radio_sky4"/>
				<radio_item label="スカイ３[ALT]" name="radio_sky3"/>
				<radio_item label="スカイ２[ALT]" name="radio_sky2"/>
				<radio_item label="地表" name="radio_sky1"/>
			</radio_group>
		</layout_panel>
		<layout_panel name="pnl_ok_cancel">
			<button label="ＯＫ" label_selected="ＯＫ" name="btn_select"/>
			<button label="キャンセル" label_selected="キャンセル" name="btn_cancel"/>
		</layout_panel>
	</layout_stack>
</floater>
