		<container_view name="joystick_monitor_axes_container">
			<!-- Zi: One stat_view for each 軸so we can hide them individually -->
			<stat_view name="axis_view_0">
				<stat_bar label="軸０" name="axis0" />
			</stat_view>

			<stat_view name="axis_view_1">
				<stat_bar label="軸１" name="axis1" />
			</stat_view>

			<stat_view name="axis_view_2">
