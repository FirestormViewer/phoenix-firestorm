     follows="left|top"
     height="16"
     increment="0.01"
     initial_value="1.0"
     layout="topleft"
     label_width="115"
     label="Active:"
     left="50"
     max_val="1.00"
     min_val="0.00"
     name="active"
     show_text="true"
     top_pad="5"
     width="415" />
    <slider
     can_edit_text="false"
     control_name="InactiveFloaterTransparency"
     decimal_digits="2"
     follows="left|top"
     height="16"
     increment="0.01"
     initial_value="0.5"
     layout="topleft"
     label_width="115"
     label="Inactive:"
     left="50"
     max_val="1.00"
     min_val="0.00"
     name="inactive"
     show_text="true"
     top_pad="2"
     width="415" />

