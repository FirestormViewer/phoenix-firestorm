         parameter="NameTagBackground" />
    </color_swatch>
    <slider
     control_name="ChatBubbleOpacity"
     follows="left|top"
     height="16"
     increment="0.05"
     initial_value="1"
     label="Opacity:"
     layout="topleft"
     left_pad="10"
     label_width="70"
     name="bubble_chat_opacity"
     tool_tip="Choose name tag opacity"
     top_delta = "6"
     width="378" />
