					<stat_bar label="Aktualna il. wychodząca" name="messagedataout"/>
				</stat_view>
			</stat_view>
			<stat_view label="Symulator" name="sim">
				<stat_bar label="Dylatacja czasu" name="simtimedilation"/>
				<stat_bar label="Klatki/sek (FPS)" name="simfps"/>
