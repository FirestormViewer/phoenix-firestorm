                    <combo_item name="high">Wysoki</combo_item>
                    <combo_item name="medium">Średni</combo_item>
                    <combo_item name="low">Niski</combo_item>
                    <combo_item name="lowest">Najniższy</combo_item>
                </combo_box>
           </panel>
           <check_box label="Brzegi" name="show_edges" />
