	</text>
	<layout_stack name="buttonstack">
		<layout_panel name="left_buttonstack">
			<button label="Auf Karte anzeigen" label_selected="Auf Karte anzeigen" name="show_on_map_btn" tool_tip="Einwohner auf Karte lokalisieren"/>
