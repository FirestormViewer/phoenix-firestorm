<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel name="teleport_history_item">
	<panel.string name="DateFmt">
		[day,datetime,utc].[mthnum,datetime,utc].[year,datetime,utc], [hour,datetime,utc]:[min,datetime,utc]
	</panel.string>
	<layout_stack name="ls_item">
		<layout_panel name="lp_region" width="165">
			<text name="region" value="..." width="165"/>
		</layout_panel>
		<layout_panel name="lp_position">
			<text name="position" value="0, 0, 0"/>
		</layout_panel>
		<layout_panel name="lp_date" width="100">
			<text name="timestamp" value="01.01.2000 12:00" width="100"/>
		</layout_panel>
	</layout_stack>
	<button name="profile_btn" tool_tip="Objektinfo anzeigen"/>
</panel>
