						<stat_bar label="Крок фізики" name="simsimphysicsstepmsec"/>
						<stat_bar label="Оновити фіз. форми" name="simsimphysicsshapeupdatemsec"/>
						<stat_bar label="Фізика - інше" name="simsimphysicsothermsec"/>
						<stat_bar label="Час сну" name="simsleepmsec"/>
						<stat_bar label="Введення/виведення" name="simpumpiomsec"/>
