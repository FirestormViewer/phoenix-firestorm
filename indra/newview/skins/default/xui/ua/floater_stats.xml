					<stat_bar label="Час на симуляцію" name="simsimothermsec"/>
					<stat_bar label="Час на клієнт" name="simagentmsec"/>
					<stat_bar label="Час на зображення" name="simimagesmsec"/>
					<stat_bar label="Час на скрипт" name="simscriptmsec"/>
					<stat_bar label="Резервний час" name="simsparemsec"/>
