<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater name="Fixed Environment" title="Фіксоване середовище">
	<string name="edit_sky">
		Редагувати налаштування неба:
	</string>
	<string name="edit_water">
		Редагувати налаштування води:
	</string>
	<layout_stack name="floater_stack">
		<layout_panel name="info_panel">
			<text name="info_panel_label">
				Ім'я:
			</text>
			<button label="Завантажити" name="btn_load" tool_tip="Завантажити налаштування з інвентарного списку"/>
			<button label="Імпорт" name="btn_import" tool_tip="Імпортувати застарілі налаштування з диска."/>
		</layout_panel>
		<layout_panel name="button_panel">
			<layout_stack name="button_bar_ls">
				<layout_panel name="save_btn_lp">
					<button label="Зберегти" name="btn_commit"/>
				</layout_panel>
				<layout_panel name="revert_btn_lp">
					<button label="Скасувати" name="btn_cancel" tool_tip="Повернутися до останньої збереженої версії"/>
				</layout_panel>
			</layout_stack>
		</layout_panel>
	</layout_stack>
</floater>
