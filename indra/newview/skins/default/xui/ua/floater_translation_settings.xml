<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater name="floater_translation_settings" title="Налаштування перекладу чату">
	<string name="azure_api_key_not_verified">
		Ідентифікатор служби Azure не підтверджено. Заголовок: [STATUS]. Перевірте налаштування та повторіть спробу.
	</string>
	<string name="google_api_key_not_verified">
		Ключ API Google не підтверджено. Заголовок: [STATUS]. Перевірте свій ключ і спробуйте ще раз.
	</string>
	<string name="deepl_api_key_not_verified">
		Ключ API DeepL не підтверджено. Заголовок: [STATUS]. Перевірте свій ключ і спробуйте ще раз.
	</string>
	<string name="azure_api_key_verified">
		Ідентифікатор служби Azure підтверджено.
	</string>
	<string name="google_api_key_verified">
		Ключ API Google підтверджено.
	</string>
	<string name="deepl_api_key_verified">
		Ключ API DeepL підтверджено.
	</string>
	<check_box label="Увімкнути машинний переклад під час чату" name="translate_chat_checkbox"/>
	<text name="translate_language_label">
		Перекладати чат на:
	</text>
	<combo_box name="translate_language_combo">
		<combo_box.item label="За замовчуванням системи" name="System Default Language"/>
		<combo_box.item label="Англійська" name="English"/>
		<combo_box.item label="Dansk (Датська)" name="Danish"/>
		<combo_box.item label="Deutsch (Немецкий)" name="German"/>
		<combo_box.item label="Español (Іспанська)" name="Spanish"/>
		<combo_box.item label="Français (Французький)" name="French"/>
		<combo_box.item label="Італійська (англійська)" name="Italian"/>
		<combo_box.item label="Magyar (Угорський)" name="Hungarian"/>
		<combo_box.item label="Nederlands (Голландська)" name="Dutch"/>
		<combo_box.item label="Polski (Польська)" name="Polish"/>
		<combo_box.item label="Português (Португальська)" name="Portugese"/>
		<combo_box.item label="Русский (російська)" name="Russian"/>
		<combo_box.item label="Turkish (Турецький)" name="Turkish"/>
		<combo_box.item label="Українська (Українська)" name="Ukrainian"/>
		<combo_box.item label="中文 (正體) (китайська)" name="Chinese"/>
		<combo_box.item label="日本語 (Японська)" name="Japanese"/>
		<combo_box.item label="한국어 (Корейський)" name="Korean"/>
	</combo_box>
	<text name="tip">
		Виберіть сервіс перекладу:
	</text>
	<radio_group name="translation_service_rg">
		<radio_item label="Перекладач Azure" name="azure" />
		<radio_item label="Перекладач Google" name="google"/>
		<radio_item label="Перекладач DeepL" name="deepl"/>
	</radio_group>
	<text name="google_links_text">
		[https://learn.microsoft.com/en-us/azure/cognitive-services/translator/create-translator-resource Установка]
	</text>
	<text name="azure_api_endoint_label">
		Кінцева точка:
	</text>
	<text name="azure_api_key_label">
		Ключ Azure:
	</text>
	<line_editor default_text="Введіть ключ Перекладача та натисніть &quot;Перевірити&quot;" name="azure_api_key"/>
	<text name="azure_api_region_label">
		регіон:
	</text>
	<line_editor default_text="Можна залишити порожнім для глобальних служб" name="azure_api_region"/>
	<button label="Перевірити" name="verify_azure_api_key_btn"/>
	<text name="google_api_key_label">
		[http://code.google.com/apis/language/translate/v2/getting_started.html#auth ключ API] Google:
	</text>
	<line_editor default_text="Введіть ключ API Google і натисніть &quot;Перевірити&quot;" name="google_api_key"/>
	<button label="Перевірити" name="verify_google_api_key_btn"/>
	<text name="google_links_text">
		[http://code.google.com/apis/language/translate/v2/pricing.html Ціни] | [https://code.google.com/apis/console Статистика]
	</text>
	<text name="deepl_api_domain_label">
		Кінцева точка:
	</text>
	<combo_box name="deepl_api_domain_combo">
		<combo_box.item label="DeepL Free" name="global"/>
		<combo_box.item label="DeepL Pro" name="api-apc"/>
	</combo_box>
	<text name="deepl_api_key_label">
		API DeepL ключ:
	</text>
	<line_editor default_text="Введіть ключ API DeepL і натисніть &quot;Перевірити&quot;" name="deepl_api_key"/>
	<button label="Перевірити" name="verify_deepl_api_key_btn"/>
	<text name="deepl_links_text">[https://www.deepl.com/pro/select-country?cta=header-prices Ціни]</text>
	<button label="OK" name="ok_btn"/>
	<button label="Скасувати" name="cancel_btn"/>
</floater>
