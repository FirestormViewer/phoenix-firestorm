<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel label="Речі" name="main inventory panel">
	<panel.string name="ItemcountFetching">
		Вибірка [ITEM_COUNT] предметів та [CATEGORY_COUNT] папок ... [FILTER]
	</panel.string>
	<panel.string name="ItemcountCompleted">
		[ITEM_COUNT] предметів(а) та [CATEGORY_COUNT] папки(ок) [FILTER]
	</panel.string>
	<panel.string name="ItemcountUnknown">
		Вибірка [ITEM_COUNT] предметів та [CATEGORY_COUNT] папок [FILTER]
	</panel.string>
	<text name="ItemcountText">
		Об'єкти:
	</text>
	<filter_editor label="Фільтр" name="inventory search editor" tool_tip="Напишіть одне або кілька слів для пошуку. Для розділення використовуйте '+' "/>
	<button name="collapse_btn" label="Згорнути"/>
	<button name="expand_btn" label="Розгорнути" />
	<text name="filter_label" width="60" value="Фільтр:"/>
	<combo_box name="filter_combo_box">
		<combo_box.item value="filter_type_all" label="Всі типи" />
		<combo_box.item value="filter_type_animations" label="Анімації" />
		<combo_box.item value="filter_type_calling_cards" label="Візитні картки" />
		<combo_box.item value="filter_type_clothing" label="Одяг / Частини тіла" />
		<combo_box.item value="filter_type_gestures" label="Жести" />
		<combo_box.item value="filter_type_landmarks" label="Закладки" />
		<combo_box.item value="filter_type_notecards" label="Нотатки" />
		<combo_box.item value="filter_type_objects" label="Об'єкти" />
		<combo_box.item value="filter_type_scripts" label="Скрипти" />
		<combo_box.item value="filter_type_sounds" label="Звуки" />
		<combo_box.item value="filter_type_textures" label="Текстури" />
		<combo_box.item value="filter_type_snapshots" label="Знімки" />
		<combo_box.item value="filter_type_meshes" label="Міш" />
		<combo_box.item value="filter_type_settings" label="Налаштування" />
		<combo_box.item value="filter_type_custom" label="Інше..." />
	</combo_box>
	<tab_container name="inventory filter tabs">
		<inventory_panel label="Інвентар" name="All Items"/>
		<recent_inventory_panel label="Недавні" name="Recent Items"/>
		<worn_inventory_panel label="Вдягнуті" name="Worn Items"/>
	</tab_container>
	<panel name="bottom_panel">
		<panel name="options_gear_btn_panel">
			<menu_button name="options_gear_btn" tool_tip="Показати додаткові налаштування"/>
		</panel>
		<panel name="add_btn_panel">
			<button name="add_btn" tool_tip="Додати новий елемент"/>
		</panel>
		<panel name="new_inventory_panel">
			<button name="new_inv_btn" tool_tip="Додаткове вікно інвентарю"/>
		</panel>
		<!-- Used by StarLight skins -->
		<panel name="collapse_panel">
			<button label="Згорнути" name="collapse_btn" tool_tip="Згорнути всі папки" />
		</panel>
		<panel name="expand_panel">
			<button label="Розгорнути" name="expand_btn" tool_tip="Розгорнути всі папки" />
		</panel>
		<!-- / Used by StarLight skins -->
		<panel name="dummy_panel">
			<text name="ItemcountText">
				Елементів
			</text>
		</panel>
		<panel name="trash_btn_panel">
			<dnd_button name="trash_btn" tool_tip="Видалити вибраний елемент"/>
		</panel>
	</panel>
</panel>
