<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater name="perm prefs" title="Стандартні дозволи">
	<panel label="Дозволи" name="permissions">
		<check_box label="Поділитися із групою" name="share_with_group"/>
		<check_box label="Дозволити всім копіювати" name="everyone_copy"/>
		<text name="NextOwnerLabel">
			Наступний власник може:
		</text>
		<check_box label="Змінювати" name="next_owner_modify"/>
		<check_box label="Копіювати" name="next_owner_copy"/>
		<check_box initial_value="true" label="перепродавати/віддавати" name="next_owner_transfer"/>
	</panel>
	<button label="ОК" label_selected="ОК" name="ok"/>
	<button label="Скасувати" label_selected="Скасувати" name="cancel"/>
</floater>
