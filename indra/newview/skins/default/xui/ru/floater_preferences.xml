<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 legacy_header_height="18"
 open_centered="true"
 default_tab_group="1"
 height="500"
 layout="topleft"
 name="Preferences"
 help_topic="preferences"
 single_instance="true"
 title="Настройки"
 width="658">
    <button
     follows="right|bottom"
     height="23"
     label="Применить"
     label_selected="Apply"
     layout="topleft"
     right="-200"
     name="Apply"
     top="467"
     width="90"
      >
        <button.commit_callback
         function="Pref.Apply" />
    </button>
    <button
     follows="right|bottom"
     height="23"
     label="Ок"
     label_selected="OK"
     layout="topleft"
     left_pad="5"
     right="-105"
     name="OK"
     width="90">
        <button.commit_callback
         function="Pref.OK" />
    </button>
    <button
     follows="right|bottom"
     height="23"
     label="Отмена"
     label_selected="Cancel"
     layout="topleft"
     left_pad="5"
     name="Cancel"
     right="-10"
     width="90" >
        <button.commit_callback
         function="Pref.Cancel" />
    </button>
