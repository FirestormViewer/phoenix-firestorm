      halign="right"
      height="16"
      layout="topleft"
      left="0"
      name="events_label"
      top_pad="16"
      width="70">
      Location:
      </text>
      <spinner
        decimal_digits="0"
        follows="right|bottom"
        height="23"
        increment="1"
        initial_value="128"
        layout="topleft"
        left_pad="4"
        max_val="8191"
        min_val="0"
        name="teleport_coordinate_x"
        width="44" >
