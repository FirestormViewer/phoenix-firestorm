    name="damage_text"
    value="100%"
    top="5"
    left="238"
    visible="true"
    width="35"
    />
     <text
	   tool_tip="Name of land parcel on which you are standing. Click for About Land."
	   text_color="White"
	   hover="true"
	   hover_color="ParcelHoverColor"
       follows="left|top|right"
       height="16"
       layout="topleft"
       left="277"
       length="1"
       name="parcel_info_text"
       top="5"
       value="(loading...)"
       type="string"
       width="100"/>
   </panel>

    <panel.string
     name="packet_loss_tooltip">
        Packet Loss
    </panel.string>
    <panel.string
     name="bandwidth_tooltip">
        Bandwidth
    </panel.string>
    <panel.string
     name="time">
        [hour12, datetime, slt]:[min, datetime, slt] [ampm, datetime, slt] [timezone,datetime, slt]
    </panel.string>
    <panel.string
     name="timeTooltip">
        [weekday, datetime, slt], [day, datetime, slt] [month, datetime, slt] [year, datetime, slt]
    </panel.string>
	<panel.string
     name="buycurrencylabel">
        L$ [AMT]
    </panel.string>
  <panel
    height="18"
    left="-385"
    width="90"
    top="0"
    follows="right|top"
    name="balance_bg"
    background_opaque="false"
    background_visible="true">
    <text
     halign="center"
     font="SansSerifSmall"
     follows="all"
     height="18"
     left="0"
	 right="24"
     name="balance"
     text_color="CurrencyColor"
     text_readonly_color="CurrencyColor"
     tool_tip="Click to refresh your L$ balance"
     v_pad="4"
     top="0"
     wrap="false"
     value="L$???"
     width="35" />
    <button
     auto_resize="true"
     halign="center"
     font="SansSerifSmall"
     follows="right|top|bottom"
     image_hover_unselected="buy_over"
     image_unselected="buy_off"
     image_pressed="buy_press"
     height="18"
     label="BUY L$"
     label_color="White"
     left_pad="2"
     label_shadow="true"
     name="buyL"
     pad_right="2"
     pad_bottom="1"
     tool_tip="Click to buy more L$"
     top="2"
     width="40" />
  </panel>
  <panel
    height="18"
    left_pad="7"
    width="130"
    top="0"
    follows="right|top"
    name="time_and_media_bg"
    background_opaque="false"
    background_visible="true">

  	<!-- Draw distance widget here -->
    <icon
     follows="right|top"
     height="16"
     image_name="Draw_Distance"
     name="dd_icon"
     tool_tip="Draw Distance"
     top="2"
     left="1"
     visible="true"
     width="16"
     scale_image="false"/>
     <slider
     control_name="RenderFarClip"
     decimal_digits="0"
     text_color="EmphasisColor" 
     follows="right|top"
     height="16"
     increment="8"
     initial_value="160"
     layout="topleft"
     tool_tip="Maximum distance away from camera to be rezzed (meters)"
     top="3"
     left_pad="2"
     max_val="1024"
     min_val="32"
     name="drawdistance" 
     top_delta="0"
     width="98" />

    <text
     type="string"
     font="SansSerifSmall"
     text_readonly_color="TimeTextColor"
     follows="right|top"
     halign="right"
     height="16"
     top="5"
     layout="topleft"
     left="135"
     name="TimeText"
     tool_tip="Current time (Pacific)"
     width="50">
        24:00 AM PST
    </text>
    <button
     follows="right|top"
     height="16"
     image_selected="Music_Off"
     image_unselected="Pause_Off"
     image_pressed="Pause_Press"
     image_pressed_selected="Music_Press"
     is_toggle="true"
     left_delta="55"
     top="1"
     name="stream_toggle_btn"
     tool_tip="Start/Stop Parcel Audio Stream"
     width="16" >
    </button>
    <button
     follows="right|top"
     height="16"
     image_selected="Play_Off"
     image_unselected="Pause_Off"
     image_pressed="Pause_Press"
     image_pressed_selected="Play_Press"
     is_toggle="true"
	 left_delta="60"
     left_pad="2"
     top="1"
     name="media_toggle_btn"
     tool_tip="Start/Stop All Media (Music, Video, Web pages)"
     width="16" >
    </button>
    <button
     follows="right|top"
     height="16"
     image_selected="AudioMute_Off"
     image_pressed="Audio_Press"
     image_unselected="Audio_Off"
     is_toggle="true"
     left_pad="4"
     top="2"
     name="volume_btn"
     tool_tip="Global Volume Control"
     width="16" />
  </panel>
</panel>
